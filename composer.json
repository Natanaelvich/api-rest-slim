{
    "name": "natanael/apirestslim",
    "description": "api rest in slim v4",
    "require": {
        "slim/psr7": "^1.0",
        "selective/basepath": "^0.2.0",
        "slim/slim": "^4.5",
        "php-di/php-di": "^6.1"
    },
    "autoload": {
        "psr-4": {
